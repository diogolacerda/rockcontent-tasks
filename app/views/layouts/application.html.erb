<!DOCTYPE html>
<html>
<head>
  <title>rockcontent Tasks</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header>
    <% if @current_user %>
      <%= render 'layouts/navigation' %>
    <% end %>
  </header>
  <main role="main">
    <%= flash_message.html_safe %>
    <%= yield %>
  </main>


</body>
</html>
